    "Sort": "ID desc",
    "PageSize": 10,
    "Span": 5
  },
  "Column": [
    {
      "Title": "ID",
      "SearchType": "Like",
      "Bind": "ID"
    },
    {
      "Title": "网点名称",
      "SearchType": "Like",
      "Bind": "siteName"
    },
    {
      "Title": "网点连接",
      "SearchType": "Like",
      "Bind": "siteLink"
    },
    {
      "Title": "入库时间",
      "SearchType": "Time",
      "BindType": "Time",
      "Bind": "AddTime"


    },
    {
      "Title": "",
      "SearchType": "Export",
      "BindType": "Html",
      "Info": "<a class='btn btn-sm btn-default' title='菜单管理' href='Jump.aspx?OutletsID={ID}'><i class='fa fa-pencil'></i> 菜单管理</a><a class='btn btn-sm btn-default' title='编辑' href='Edit.aspx?ID={ID}'><i class='fa fa-pencil'></i> 编辑/查看</a><a class='btn btn-sm btn-default lab456' onclick='if(confirm(\"确认删除吗？\"))SetIsDel([{ID},1])' title='删除'><i class='glyphicon glyphicon-remove'></i> 删除</a>"
    }
  ],
  "Ajax": [

    {
      "Name": "SetIsDel",
      "SQL": "Update Admin_OutletsManage set IsDel=@1 where ID=@0",
